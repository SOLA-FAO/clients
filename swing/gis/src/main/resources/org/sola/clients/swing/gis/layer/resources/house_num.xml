<?xml version="1.0" encoding="UTF-8"?>
<sld:UserStyle xmlns="http://www.opengis.net/sld" xmlns:sld="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc" xmlns:gml="http://www.opengis.net/gml">
    <sld:Name>House Number</sld:Name>
    <sld:Title/>
    <sld:FeatureTypeStyle>
        <sld:Name>name</sld:Name>
        <sld:FeatureTypeName>Feature</sld:FeatureTypeName>
        <sld:Rule>
            <sld:Name>&lt; 10,000</sld:Name>
            <sld:MaxScaleDenominator>10000.0</sld:MaxScaleDenominator>
            <!--sld:PolygonSymbolizer>
                <sld:Stroke>
                    <sld:CssParameter name="stroke">#339900</sld:CssParameter>
                    <sld:CssParameter name="stroke-width">2.0</sld:CssParameter>
                </sld:Stroke>
            </sld:PolygonSymbolizer-->
            <sld:TextSymbolizer>
                <sld:Label>
                    <ogc:PropertyName>label</ogc:PropertyName>
                </sld:Label>
                <sld:Font>
                    <sld:CssParameter name="font-family">Arial</sld:CssParameter>
                    <sld:CssParameter name="font-size">12.0</sld:CssParameter>
                    <sld:CssParameter name="font-style">normal</sld:CssParameter>
                    <sld:CssParameter name="font-weight">normal</sld:CssParameter>
                </sld:Font>
                <sld:LabelPlacement>
                    <sld:PointPlacement>
                        <sld:AnchorPoint>
                            <sld:AnchorPointX>0.5</sld:AnchorPointX>
                            <sld:AnchorPointY>0.5</sld:AnchorPointY>
                        </sld:AnchorPoint>
                        <sld:Displacement>
                            <sld:DisplacementX>0.0</sld:DisplacementX>
                            <sld:DisplacementY>0.0</sld:DisplacementY>
                        </sld:Displacement>
                    </sld:PointPlacement>
                </sld:LabelPlacement>
                <sld:Halo>
                    <sld:Radius>1.0</sld:Radius>
                    <sld:Fill>
                        <sld:CssParameter name="fill">#FFFFFF</sld:CssParameter>
                        <sld:CssParameter name="fill-opacity">0.5</sld:CssParameter>
                    </sld:Fill>
                </sld:Halo>
                <sld:Fill>
                    <sld:CssParameter name="fill">#FF7F50</sld:CssParameter>
                </sld:Fill>
                <sld:VendorOption name="group">false</sld:VendorOption>
            </sld:TextSymbolizer>
        </sld:Rule>
    </sld:FeatureTypeStyle>
</sld:UserStyle>
